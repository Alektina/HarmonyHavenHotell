    <script>(function () {
            var $slide_rets = document.querySelectorAll('.slide_ret');
            var $controls = document.querySelectorAll('.slide_retr__control');
            var $linesikderfull_ret = document.querySelectorAll('.linesikderfull_ret');
            var numOfslide_rets = $slide_rets.length;
            var slidingAT = 1300;
            var slidingBlocked = false;

            [].slice.call($slide_rets).forEach(function ($el, index) {
                var i = index + 1;
                $el.classList.add('slide_ret-' + i);
                $el.dataset.slide_ret = i;
            });

            [].slice.call($controls).forEach(function ($el) {
                $el.addEventListener('click', controlClickHandler);
            });

            function controlClickHandler() {
                if (slidingBlocked) return;
                slidingBlocked = true;
                var $control = this;
                var isRight = $control.classList.contains('m--right');
                var $curactive_ret = document.querySelector('.slide_ret.s-ret-active_ret');
                var index = +$curactive_ret.dataset.slide_ret;

                (isRight) ? index++ : index--;
                if (index < 1) index = numOfslide_rets;
                if (index > numOfslide_rets) index = 1;

                var $newactive_ret = document.querySelector('.slide_ret-' + index);
                $control.classList.add('a--rotation');
                $curactive_ret.classList.remove('s-ret-active_ret', 's-ret-active_ret-prev');
                document.querySelector('.slide_ret.s-ret-prev').classList.remove('s-ret-prev');
                $newactive_ret.classList.add('s-ret-active_ret');
                if (!isRight) $newactive_ret.classList.add('s-ret-active_ret-prev');
                var prevIndex = index - 1;
                if (prevIndex < 1) prevIndex = numOfslide_rets;
                document.querySelector('.slide_ret-' + prevIndex).classList.add('s-ret-prev');

                // Update linesikderfullret width based on current slide
                var linesikderWidth_ret = 100 + 100 * (index - 1);
                $linesikderfull_ret.forEach(function ($el) {
                    $el.style.width = linesikderWidth_ret + 'px';
                });

                setTimeout(function () {
                    $control.classList.remove('a--rotation');
                    slidingBlocked = false;
                }, slidingAT * 0.75);
            };
        }());</script>

