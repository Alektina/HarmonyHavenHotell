    <script>const slide = document.getElementById('image_slide');
        const image = document.getElementsByClassName('image');

        window.onmousedown = e => {
            slide.dataset.mouseDownAt = e.clientX;
        }

        window.onmousemove = e => {
            if (slide.dataset.mouseDownAt === "0") return;
            const mouseAlpha = parseFloat(slide.dataset.mouseDownAt - e.clientX);
            const maxAlpha = window.innerWidth / 2;

            var percentage = (mouseAlpha / maxAlpha) * 80; //we need a percentage value to use at our css file.
            var nextPercentage = parseFloat(slide.dataset.slidePrevPercentage) + percentage;
            slide.dataset.slidePercentage = nextPercentage;


            if (nextPercentage < 0) {
                nextPercentage = 0;
                percentage = 0;
            }
            if (nextPercentage > 100) {
                nextPercentage = 100;
                percentage = 100;
            }
            console.log('percentage', percentage);
            console.log('nextPercentage', nextPercentage);

            //   animate the slide
            slide.animate({
                transform: `translate(-${nextPercentage}%, -50%)`
            }, { duration: 1000, fill: 'forwards' });

            //   the paralax effect for the image:
            for (let i = 0; i < image.length; i++) {
                image[i].animate({
                    objectPosition: `${nextPercentage}% center`
                }, { duration: 1000, fill: 'forwards' });
            }
        }
        window.onmouseup = () => {
            slide.dataset.mouseDownAt = "0";
            slide.dataset.slidePrevPercentage = slide.dataset.slidePercentage;
        }

        $(document).ready(function () {
            $(".filter_btn").on("click", function () {
                var category = $(this).data("category");

                // Show images based on the selected category
                if (category === "all") {
                    $(".image").show();
                } else {
                    $(".image").hide();
                    $("." + category).show();
                }

                // Toggle the "active" class on the clicked button
                $(".filter_btn").removeClass("active");
                $(this).addClass("active");
            });
        });</script>

