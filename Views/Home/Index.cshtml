            <script>

              
                // Booking form

                // JavaScript to populate the dropdown with dates for the next year
                var arrivalDropdown = document.getElementById('arrivalDateDropdown');
                var departureDropdown = document.getElementById('departureDateDropdown');
                var arrivalDropbtn = document.getElementById('arrivalDropbtn');
                var departureDropbtn = document.getElementById('departureDropbtn');
                var startDate = new Date();
                var endDate = new Date();
                endDate.setDate(startDate.getDate() + 7); // Date range for the next week

                // Function to set the selected date as the button text in the desired format
                function setDateText(dropbtn, date) {
                    // Format the date as "DD Month YYYY"
                    var formattedDate = date.getDate() + ' ' + getMonthName(date.getMonth()) + ' ' + date.getFullYear();
                    dropbtn.textContent = formattedDate;
                }

                // Function to get the month name from the month index
                function getMonthName(monthIndex) {
                    var months = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
                    return months[monthIndex];
                }

                // Function to handle date selection
                function selectDate(dropbtn, date) {
                    setDateText(dropbtn, date);
                }

                function populateDropdown(dropdown, dropbtn) {
                    var currentDate = new Date(startDate);
                    while (currentDate <= endDate) {
                        var option = document.createElement('p');
                        // Format the date as "DD Month YYYY"
                        option.textContent = currentDate.getDate() + ' ' + getMonthName(currentDate.getMonth()) + ' ' + currentDate.getFullYear();
                        option.onclick = function () {
                            var selectedDate = new Date(this.textContent);
                            selectDate(dropbtn, selectedDate);
                        };
                        dropdown.appendChild(option);
                        currentDate.setDate(currentDate.getDate() + 1);
                    }
                }

                populateDropdown(arrivalDropdown, arrivalDropbtn);
                populateDropdown(departureDropdown, departureDropbtn);

                // Set initial button text to current day in the desired format
                setDateText(arrivalDropbtn, startDate);
                setDateText(departureDropbtn, startDate);

                // JavaScript to handle dropdown functionality
                document.addEventListener('DOMContentLoaded', function () {
                    const dropdowns = document.querySelectorAll('.dropdown');
                    dropdowns.forEach(dropdown => {
                        const button = dropdown.querySelector('.dropbtn_light');
                        const menu = dropdown.querySelector('.dropdown-content');

                        menu.addEventListener('click', function (e) {
                            const selectionClass = e.target.classList[0];
                            if (selectionClass) {
                                button.textContent = e.target.textContent;
                            }
                        });
                    });
                });</script>

