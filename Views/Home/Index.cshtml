    <script>(function () {
            // Selecting necessary elements
            var $slides = document.querySelectorAll('.slide'); // All slides
            var $controls = document.querySelectorAll('.slider__control'); // Control buttons
            var $linesikderfull = document.querySelectorAll('.linesikderfull'); // Line indicator elements
            var numOfSlides = $slides.length; // Total number of slides
            var slidingAT = 1300; // Sliding animation duration
            var slidingBlocked = false; // Flag to prevent overlapping animations

            // Adding classes and data attributes to slides for identification
            [].slice.call($slides).forEach(function ($el, index) {
                var i = index + 1;
                $el.classList.add('slide-' + i);
                $el.dataset.slide = i;
            });

            // Adding event listeners to control buttons
            [].slice.call($controls).forEach(function ($el) {
                $el.addEventListener('click', controlClickHandler);
            });

            // Function to handle control button clicks
            function controlClickHandler() {
                // Preventing multiple click events during sliding animation
                if (slidingBlocked) return;
                slidingBlocked = true;

                // Determining the direction of slide (left or right)
                var $control = this;
                var isRight = $control.classList.contains('m--right');

                // Selecting current active slide and its index
                var $curActive = document.querySelector('.slide.s--active');
                var index = +$curActive.dataset.slide;

                // Updating slide index based on the direction
                (isRight) ? index++ : index--;
                if (index < 1) index = numOfSlides;
                if (index > numOfSlides) index = 1;

                // Selecting the next active slide
                var $newActive = document.querySelector('.slide-' + index);

                // Adding rotation animation class to control button
                $control.classList.add('a--rotation');

                // Removing active classes from current slide and its previous slide
                $curActive.classList.remove('s--active', 's--active-prev');
                document.querySelector('.slide.s--prev').classList.remove('s--prev');

                // Adding active classes to the next slide and its previous slide
                $newActive.classList.add('s--active');
                if (!isRight) $newActive.classList.add('s--active-prev');
                var prevIndex = index - 1;
                if (prevIndex < 1) prevIndex = numOfSlides;
                document.querySelector('.slide-' + prevIndex).classList.add('s--prev');

                // Update linesikderfull width based on current slide
                var linesikderWidth = 100 + 100 * (index - 1);
                $linesikderfull.forEach(function ($el) {
                    $el.style.width = linesikderWidth + 'px';
                });

                // Reset slidingBlocked flag after sliding animation completes
                setTimeout(function () {
                    $control.classList.remove('a--rotation');
                    slidingBlocked = false;
                }, slidingAT * 0.75);
            }
        })();</script>
